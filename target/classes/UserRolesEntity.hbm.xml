<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mkyong.web.UserRolesEntity" table="user_roles" schema="army">
        <id name="userRoleId">
            <column name="user_role_id" sql-type="int(11)"/>
        </id>
        <property name="role">
            <column name="role" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>