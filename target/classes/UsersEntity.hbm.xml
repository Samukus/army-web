<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mkyong.web.UsersEntity" table="users" schema="army">
        <id name="username">
            <column name="username" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="enabled">
            <column name="enabled" sql-type="tinyint(4)"/>
        </property>
        <property name="fname">
            <column name="fname" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="sname">
            <column name="sname" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="tname">
            <column name="tname" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date" not-null="true"/>
        </property>
        <property name="workRole">
            <column name="work_role" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="category">
            <column name="category" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(1024)" length="1024" not-null="true"/>
        </property>
        <property name="rank">
            <column name="rank" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(16)" length="16" not-null="true"/>
        </property>
        <property name="adress">
            <column name="adress" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>